private int num=0;
private int  max =0;
private int  min =0;
public void  GetAnumber(int lo,int hi )
{

num = (int)(Math.floor(Math.random()*((hi-lo)+1)+lo);

}
 
public void SetLowNumber(int low)
{
min = low;
}

public void SetHighNumber(int high)
{
max =high  ; 
}

public int GetNumber()
{
GetNumber(min,max);
return num;
}
